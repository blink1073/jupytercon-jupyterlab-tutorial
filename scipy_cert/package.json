{
  "name": "jupytercon_cert",
  "version": "0.1.0",
  "description": "A JupyterLab extension for rendering cert",
  "author": "matthias bussonnier <foo@foo.com>",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/*.css"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tcs -w",
    "extension:install": "jupyter labextension install jupytercon_cert",
    "extension:uninstall": "jupyter labextension uninstall  jupytercon_cert",
    "extension:enable": "jupyter labextension enable jupytercon_cert",
    "extension:disable": "jupyter labextension disable jupytercon_cert"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^1.0.0",
    "@phosphor/widgets": "^1.5.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "~2.9.2"
  }
}
